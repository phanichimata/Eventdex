         for(var k=0; k<mycars.length; k++){
           var  selectedIdInputclass =  mycars[k]+selectedId; 
            var  destinationClass = mycars[k]+selectedValue;
            
            for(var i=0; i<inputElem.length; i++){     
                if(inputElem[i].className == destinationClass){     
