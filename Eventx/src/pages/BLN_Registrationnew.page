<script>

  function validateRequiredFields(){
     
   var tablelist = document.getElementsByClassName('fields');
    var isdatavalid = true;
    
    for(var t=0;t<tablelist.length;t++){
       var trlist = tablelist[t].getElementsByTagName('tr');
       for(var trl=0; trl< trlist.length ; trl++){
         var tdlist = trlist[trl].getElementsByTagName('td');
          if(tdlist.length >=2){
            if(tdlist[0].innerHTML.indexOf('*') !== -1){
                var nesttablist=  tdlist[1].getElementsByTagName('table'); 
                 
                    if(nesttablist.length<=0){
                      var tdinput = tdlist[1].getElementsByTagName('input');
                       
                           for(var tdin = 0; tdin < tdinput.length; tdin++){
                            
                            if(tdinput[tdin].value=="" ){
                              tdinput[tdin].style.borderColor = "red";
                              tdinput[tdin].style.boxShadow = "#A63737 1px 2px 4px 1px";
                               
                              isdatavalid = false;
                            } else{
                              
                               tdinput[tdin].style.border = "1px solid #CCCCCC";
                            }                 
                         }
                    }else{
                       
                        for(var nst=0; nst< nesttablist.length; nst++ ){
                           var chkrdp =  nesttablist[0].getElementsByTagName('input');
                           var isselected = false;
                           for(var ch=0;ch < chkrdp.length ;ch++){
                              
                             if(chkrdp[ch].checked){
                                  isselected =true;
                                  break;
                               }
                           }
                          
                        if(isselected){
                        nesttablist[nst].style.backgroundColor ="#ccc";
                      } else{
                         nesttablist[nst].style.backgroundColor ="red";
                          isdatavalid = false;
                         
                      }                      
                     } 
                   }
                
               var tdselect = tdlist[1].getElementsByTagName('select');
               for(var tdse = 0; tdse < tdselect.length; tdse++){
                   var selectlist =  tdselect[tdse].options[tdselect[tdse].selectedIndex].value;
                   if(selectlist=="" || selectlist == "a10c0000004QjxtAAC" ||  selectlist == "a1Oc00000026YSZEA2"){
                        tdselect[tdse].style.borderColor = "red";
                          tdselect[tdse].style.boxShadow = "#A63737 1px 2px 4px 1px";
                          isdatavalid = false;
                   } else{
                     tdselect[tdse].style.border = "1px solid #CCCCCC";
                   } 
               }
               
            }
          }
          
       }
    }
     return isdatavalid ;
   }
</script>

